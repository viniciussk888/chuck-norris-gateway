{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [
      {
        "include": "**/*.pem",
        "outDir": "dist/src"
      }
    ]
  },
  "entryFile": "src/index.js"
}
